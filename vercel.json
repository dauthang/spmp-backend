{
    "version": 2,
    "builds":[
        {
            "src":"src/main.ts",
            "use":"package.json"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/main.ts",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}