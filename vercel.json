{
    "version": 2,
    "builds":[
        {
            "src":"src/main.ts",
            "use":"@vercel/node"
        }
    ],
    "routers":[
        {
            "src":"/(.*)",
            "dest":"src/main.ts",
            "method":[
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}